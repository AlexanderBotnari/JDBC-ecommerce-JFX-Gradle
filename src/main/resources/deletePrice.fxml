<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.PriceController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <Label alignment="CENTER" layoutX="128.0" layoutY="53.0" prefHeight="31.0" prefWidth="350.0" style="&#10;" text="Enter id for delete Price" textAlignment="CENTER" wrapText="false">
          <font>
            <Font size="18.0" />
          </font>
        </Label>
            <TextField fx:id="id" layoutX="161.0" layoutY="103.0" prefHeight="51.0" prefWidth="284.0" promptText="id" />
            <Button layoutX="189.0" layoutY="200.0" mnemonicParsing="false" onAction="#deletePrice" prefHeight="27.0" prefWidth="61.0" text="Enter" textFill="#006f11" />
            <Button layoutX="343.0" layoutY="200.0" mnemonicParsing="false" onAction="#logout" text="Cancel" textFill="#ff0404" />
            <Label fx:id="message" layoutX="124.0" layoutY="251.0" prefHeight="89.0" prefWidth="426.0" textFill="#b20e0e">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
      </children>
    </AnchorPane>
  </children>
</VBox>
