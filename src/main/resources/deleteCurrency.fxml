<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.CurrencyController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <Label alignment="CENTER" layoutX="144.0" layoutY="43.0" prefHeight="41.0" prefWidth="306.0" style="&#10;" text="Delete currency where id" textAlignment="CENTER" wrapText="false">
          <font>
            <Font size="24.0" />
          </font>
        </Label>
            <TextField fx:id="id" layoutX="160.0" layoutY="91.0" prefHeight="57.0" prefWidth="274.0" promptText="id" />
            <Button layoutX="196.0" layoutY="198.0" mnemonicParsing="false" onAction="#deleteCurrency" prefHeight="27.0" prefWidth="61.0" text="Enter" textFill="#15d715" />
            <Button layoutX="327.0" layoutY="198.0" mnemonicParsing="false" onAction="#logout" text="Cancel" textFill="#ae1010" />
            <Label fx:id="message" layoutX="144.0" layoutY="265.0" prefHeight="87.0" prefWidth="384.0" textFill="#c90606">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
      </children>
    </AnchorPane>
  </children>
</VBox>
