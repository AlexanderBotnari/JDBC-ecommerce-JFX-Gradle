<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ProductController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <Label alignment="CENTER" layoutX="161.0" layoutY="51.0" prefHeight="23.0" prefWidth="301.0" style="&#10;" text="Enter id to find product!!!" textAlignment="CENTER" textFill="#0d0d0d" wrapText="false">
          <font>
            <Font size="18.0" />
          </font>
        </Label>
            <TextField fx:id="id" layoutX="210.0" layoutY="110.0" prefHeight="43.0" prefWidth="220.0" promptText="id" />
            <Button layoutX="161.0" layoutY="212.0" mnemonicParsing="false" onAction="#findByIdProduct" prefHeight="27.0" prefWidth="111.0" text="OK" textFill="#00a84e" />
            <Button layoutX="358.0" layoutY="212.0" mnemonicParsing="false" onAction="#logout" prefHeight="27.0" prefWidth="111.0" text="CANCEL" textFill="#f50101" />
            <Label fx:id="message" layoutX="120.0" layoutY="290.0" prefHeight="63.0" prefWidth="383.0" textAlignment="CENTER" textFill="#024fa1">
               <font>
                  <Font name="Arial Black" size="13.0" />
               </font>
            </Label>
            <Label layoutX="120.0" layoutY="273.0" prefHeight="17.0" prefWidth="111.0" text="Result:" />
      </children>
    </AnchorPane>
  </children>
</VBox>
