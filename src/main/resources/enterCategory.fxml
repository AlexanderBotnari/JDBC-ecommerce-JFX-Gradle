<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.CategoryController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <Label alignment="CENTER" layoutX="192.0" layoutY="43.0" prefHeight="43.0" prefWidth="257.0" style="&#10;" text="Add category a Data Base" textAlignment="CENTER" wrapText="false">
          <font>
            <Font size="18.0" />
          </font>
        </Label>
            <TextField fx:id="id" layoutX="176.0" layoutY="112.0" prefHeight="57.0" prefWidth="286.0" promptText="id" />
            <TextField fx:id="name" layoutX="176.0" layoutY="200.0" prefHeight="57.0" prefWidth="286.0" promptText="name" />
            <Button layoutX="212.0" layoutY="284.0" mnemonicParsing="false" onAction="#createCategory" prefHeight="27.0" prefWidth="80.0" text="OK" />
            <Button layoutX="361.0" layoutY="284.0" mnemonicParsing="false" onAction="#logout" prefHeight="27.0" prefWidth="80.0" text="Cancel" />
            <Label fx:id="message" layoutX="146.0" layoutY="324.0" prefHeight="57.0" prefWidth="410.0" textFill="#0c9000">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
      </children>
    </AnchorPane>
  </children>
</VBox>
