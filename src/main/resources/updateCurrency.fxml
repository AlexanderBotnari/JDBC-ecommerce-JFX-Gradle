<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.CurrencyController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <Label alignment="CENTER" layoutX="154.0" layoutY="32.0" prefHeight="42.0" prefWidth="333.0" style="&#10;" text="Update Currency with id" textAlignment="CENTER" textFill="#030303" wrapText="false">
          <font>
            <Font size="18.0" />
          </font>
        </Label>
            <TextField fx:id="id" layoutX="175.0" layoutY="80.0" prefHeight="42.0" prefWidth="280.0" promptText="id" />
            <TextField fx:id="code" layoutX="174.0" layoutY="142.0" prefHeight="42.0" prefWidth="280.0" promptText="new code" />
            <TextField fx:id="rate" layoutX="174.0" layoutY="209.0" prefHeight="42.0" prefWidth="280.0" promptText="new rate" />
            <Button layoutX="205.0" layoutY="274.0" mnemonicParsing="false" onAction="#updateCurrency" prefHeight="27.0" prefWidth="61.0" text="Enter" textFill="#127216" />
            <Button layoutX="340.0" layoutY="274.0" mnemonicParsing="false" onAction="#logout" text="Cancel" textFill="#dd1717" />
            <Label fx:id="message" layoutX="117.0" layoutY="314.0" prefHeight="62.0" prefWidth="440.0" textFill="#009013">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
      </children>
    </AnchorPane>
  </children>
</VBox>
