<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.PriceController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <Label alignment="CENTER" layoutX="146.0" layoutY="52.0" prefHeight="37.0" prefWidth="310.0" style="&#10;" text="Enter id for find price" textAlignment="CENTER" textFill="#171717" wrapText="false">
          <font>
            <Font size="18.0" />
          </font>
        </Label>
            <TextField fx:id="id" layoutX="138.0" layoutY="114.0" prefHeight="59.0" prefWidth="304.0" promptText="id" />
            <Button layoutX="178.0" layoutY="207.0" mnemonicParsing="false" onAction="#findByIdPrice" prefHeight="27.0" prefWidth="65.0" text="Enter" />
            <Button layoutX="320.0" layoutY="207.0" mnemonicParsing="false" onAction="#logout" prefHeight="27.0" prefWidth="65.0" text="Cancel" />
            <Label fx:id="message" layoutX="50.0" layoutY="271.0" prefHeight="84.0" prefWidth="522.0" textFill="#249402" />
      </children>
    </AnchorPane>
  </children>
</VBox>
