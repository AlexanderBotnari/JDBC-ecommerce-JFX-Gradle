<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.CurrencyController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <Label alignment="CENTER" layoutX="150.0" layoutY="29.0" prefHeight="65.0" prefWidth="318.0" style="&#10;" text="Enter id for find currency" textAlignment="CENTER" wrapText="false">
          <font>
            <Font size="18.0" />
          </font>
        </Label>
            <TextField fx:id="id" layoutX="156.0" layoutY="103.0" prefHeight="49.0" prefWidth="298.0" promptText="id" />
            <Button layoutX="192.0" layoutY="215.0" mnemonicParsing="false" onAction="#findByIdCurrency" prefHeight="27.0" prefWidth="61.0" text="Enter" textFill="#02a802" />
            <Button layoutX="349.0" layoutY="215.0" mnemonicParsing="false" onAction="#logout" text="Cancel" textFill="#da0404" />
            <Label fx:id="message" layoutX="98.0" layoutY="273.0" prefHeight="77.0" prefWidth="483.0" textFill="#05c63f">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
      </children>
    </AnchorPane>
  </children>
</VBox>
