<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.CategoryController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <Label alignment="CENTER" layoutX="150.0" layoutY="67.0" prefHeight="23.0" prefWidth="309.0" style="&#10;" text="Enter id for delete category" textAlignment="CENTER" textFill="#2c2a2a" wrapText="false">
          <font>
            <Font size="23.0" />
          </font>
        </Label>
            <TextField fx:id="id" layoutX="187.0" layoutY="117.0" prefHeight="42.0" prefWidth="236.0" promptText="id" />
            <Button layoutX="177.0" layoutY="213.0" mnemonicParsing="false" onAction="#deleteCategory" prefHeight="27.0" prefWidth="99.0" text="OK" textFill="#018077" />
            <Button layoutX="320.0" layoutY="213.0" mnemonicParsing="false" onAction="#logout" prefHeight="27.0" prefWidth="99.0" text="Cancel" textFill="#a10000" />
            <Label fx:id="message" layoutX="125.0" layoutY="300.0" prefHeight="56.0" prefWidth="438.0" textFill="#208a00">
               <font>
                  <Font size="18.0" />
               </font></Label>
      </children>
    </AnchorPane>
  </children>
</VBox>
